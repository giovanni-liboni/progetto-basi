<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="util.InfoPrenotazioneBean" table="prenotazione">
        <!-- Identificatore. -->
        <id name="codicevolo" column="codicevolo"/>
        <!-- Proprieta'. -->
        <property name="partenza" column="partenza" not-null="true"/>    
        <property name="arrivo" column="arrivo" not-null="true"/>
        <property name="documento" column="documento" not-null="true"/>
        <property name="datarichiesta" column="datarichiesta" not-null="true"/>
    	<property name="orarichiesta" column="orarichiesta" not-null="true"/>
    	
    	<property name="datapartenza" column="datapartenza" not-null="true"/>
    	<property name="orapartenza" column="orapartenza" not-null="true"/>
    </class>
</hibernate-mapping>