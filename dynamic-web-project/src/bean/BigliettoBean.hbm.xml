<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="bean.BigliettoBean" table="biglietto">
        <!-- Identificatore. -->
        <composite-id>
        	<key-property name="documento"/>
        	<key-property name="codicevolo"/>
        </composite-id>
        <!-- Proprieta' -->
        <property name="prezzo" column="prezzo" not-null="true"/>    
   		<property name="dataemissione" column="dataemissione" not-null="true"/>
   		<set name="id_prenotazione" table="prenotazione" lazy="true" fetch="select">
   			<key column="id"/>
   		</set>
    </class>
</hibernate-mapping>