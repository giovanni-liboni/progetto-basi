 <?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="util.TrattaBean" table="tratta">
        <!-- Identificatore. -->
        <id name="partenza"/>
        <key name="arrivo"/>
        <!-- Proprieta'. -->
        <property name="durata" not-null="true"/>
        <property name="distanza" not-null="true"/>
    </class>
</hibernate-mapping>