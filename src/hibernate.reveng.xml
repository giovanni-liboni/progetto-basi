		not-null="true">
	</sql-type>
	<sql-type jdbc-type="DOUBLE" hibernate-type="double" precision="17"
		scale="17" not-null="true">
	</sql-type>
	<sql-type jdbc-type="DATE" hibernate-type="date" length="13"
		not-null="true">
	</sql-type>
	<sql-type jdbc-type="VARCHAR" hibernate-type="string" length="10"
		not-null="true">
	</sql-type>
