		not-null="true">
	</sql-type>
	<sql-type jdbc-type="BIT" hibernate-type="java.lang.Boolean"
		not-null="false">
	</sql-type>
	<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
	<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
	<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
	<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
	<sql-type jdbc-type="VARCHAR" hibernate-type="string"></sql-type>
	<sql-type jdbc-type="DATE" hibernate-type="date" length="13"
		not-null="true">
	</sql-type>
</type-mapping>
<table-filter match-schema="public" match-name=".*" />
<table schema="public" name="imbarco" class="imbarco">
